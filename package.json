{
  "name": "issue-fe-multi-select",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-contact-functions": "cds watch --open contactfunctions/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-customer-accounts": "cds watch --open customeraccounts/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-cores": "cds watch --open cores/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/contact-functions",
    "app/customer-accounts",
    "app/cores"
  ]
}
